{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-globals": ["error", "event", "fdescribe"],
    "no-underscore-dangle": [0],
    "no-console": [0],
    "no-param-reassign": [0],
    "no-alert": [0],
    "consistent-return": [0],
    "no-prototype-builtins": [0]
  }
}
